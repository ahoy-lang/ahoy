? Raylib Window Example in Ahoy
# This demonstrates importing C libraries and using them in our language

import "raylib.h"

screen_width: 800; screen_height: 450

init_window|screen_width, screen_height, "Ahoy + Raylib - Basic Window"|
SetTargetFPS|60|

x: 100; y: 100; speed: 2

loop not window_should_close|| then
    if is_key_down|KEY_RIGHT| then
        x: x plus speed
    if is_key_down|KEY_LEFT| then
        x: x minus speed
    if is_key_down|KEY_DOWN| then
        y: y plus speed
    if is_key_down|KEY_UP| then
        y: y minus speed

    begin_drawing||
    clear_background|RAYWHITE|
    draw_text|"Move the circle with arrow keys!", 200, 20, 20, DARKGRAY|
    draw_circle|x, y, 50, MAROON|
    DrawFPS|10, 10|
    end_drawing||

    new_func :: |adsklfjlk, adlkfjkldasj| float, bool then
				ahoy|"Inside new_func: %f and %b\n", adsklfjlk, adlkfjkldasj|

    asdlkfjklds :: |kjakldsjf| float,bool then

    	ahoy||

close_window||
