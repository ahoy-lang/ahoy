? Arrays test
numbers: [1, 2, 3, 4, 5]
first: numbers[0]
print|first| ? should print: 1

words: ["hello", "world", "ahoy"]
word: words[1]
print|word| ? should print: world

items: [10, 20, 30, 40]
len: items.length||
print|len| ? should print: 4

arr: [5, 6, 7, 8]
last: arr.pop||
print|last| ? should print: 8

values: [2, 4, 6, 8]
total: values.sum||
print|total| ? should print: 20

nums: [10, 20, 30]
has_20: nums.has|20|
print|has_20| ? should print: 1 (true)

original: [1, 2, 3]
reversed: original.reverse||
third: reversed[2]
print|third| ? should print: 1

