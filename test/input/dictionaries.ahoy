? Dictionaries test
person: {"name": "Alice", "city": "NYC"}
name: person{"name"}
print|name| ? should print: Alice

city: person{"city"}
print|city| ? should print: NYC

data: {"a": "first", "b": "second", "c": "third"}
size: data.size||
print|size| ? should print: 3

config: {"theme": "dark"}
has_theme: config.has|"theme"|
print|has_theme| ? should print: 1 (true)

has_font: config.has|"font"|
print|has_font| ? should print: 0 (false)

settings: {"mode": "dev"}
settings{"port"}: "8080"
port: settings{"port"}
print|port| ? should print: 8080

