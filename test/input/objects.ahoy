import "raylib.h"
expected: []
struct particle:
  position: vector2,
  velocity: vector2,
  rotation: float
  type smoke_particle:
	  10 size: float, ? initial value set to 10.0
		1.0 alpha: float, ? initial value set to 1.0
	 	1 life: float, ? initial value set to 1.0
		max_life: float, ? initial value set to 0.0
		name: string, ? initial value set to ""
		color: color
  type wind_particle:
    <10,20> direction: vector2,
    speed: float,
    size: vector2
$

smoke_particle_instance: smoke_particle<position:vector2|120,390|, size: 10.0, alpha: 1.0, life:1.0>
print|smoke_particle_instance|
expected.push|"smoke_particle<position:vector2(120, 390), velocity:vector2(0, 0), rotation:0.0, size:10, alpha:1, life:1, max_life:0.0, name:\"\", color:color(0, 0, 0, 0)>"|
particle_instance: particle<position:vector2|0,0|, velocity:vector2|1,1|, rotation:0.0>
print|particle_instance|
expected.push|"smoke_particle<position:vector2(120, 390), velocity:vector2(0, 0), rotation:0.0, size:10, alpha:1, life:1, max_life:0.0, name:\"\", color:color(0, 0, 0, 0)>"|
wind_particle_instance : wind_particle<speed:5.0, size:vector2|10,10|>
print|wind_particle_instance|
expected.push|"wind_particle<position:vector2(0, 0), velocity:vector2(0, 0), rotation:0.0, direction:vector2(10,20), speed:5.0, size:vector2(10, 10)>"|


? accessing object struct properties
smoke_particle1: smoke_particle<>
smoke_particle1.max_life: 1.0
smoke_particle1<'max_life'>: 100.0
smoke_particle1.position.x: 1.0
smoke_particle1.position.y: 1.0
smoke_particle1<'position.x'>: 100.0
print|smoke_particle1|
expected.push|"smoke_particle<position:vector2(100, 0), velocity:vector2(0, 0), rotation:0.0, size:10, alpha:1, life:0.0, max_life:100, name:\"\", color:color(0, 0, 0, 0)>"|


? Objects literals test
person: <name: "Alice", age: 30>
print|person| ? should print: {name: "Alice", age: 30}
expected.push|"{name: \"Alice\", age: 30}"|
print|person.name| ? should print: Alice
expected.push|"Alice"|
print|person.age| ? should print: 30
expected.push|"30"|

data: <label: "Test", value: 3.14>
print|data.label| ? should print: Test
expected.push|"Test"|

config: <host: "localhost", port: 8080>
print|config.host| ? should print: localhost
expected.push|"localhost"|

? Print expected values for test validation
print|expected|
